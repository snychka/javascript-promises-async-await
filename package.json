{
  "name": "refactoring-promises-async-await",
  "version": "1.0.0",
  "description": "Refactoring JavaScript Promises with Async/Await",
  "main": "index.js",
  "scripts": {
    "test": "npm-run-all -s test:code",
    "test:code": "mocha test/",
    "clean": "rimraf build",
    "go": "npm-run-all build exec",
    "exec": "node ./build/index.js",
    "build": "npm-run-all clean transpile",
    "transpile": "babel ./src --out-dir build --copy-files",
    "deploy:github-pages": "git push origin master:gh-pages",
    "test-watch": "npm-run-all  -p -r test-watch:code",
    "test-watch:code": "mocha test/ --bail --watch --watch-extensions html"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "esmodules": true
          }
        }
      ]
    ]
  },
  "author": "Marques Woodson",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "acorn": "^7.1.0",
    "chai": "^4.2.0",
    "jquery": "^3.3.1",
    "jsdom": "^11.12.0",
    "mocha": "^7.0.1",
    "npm-run-all": "^4.0.2",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0"
  }
}